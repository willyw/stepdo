<% form_for post, :html=>{:class=>"post_title"} do |f| %>
	<%= f.text_field :title , :id => ""%>
		<input type="hidden" id="secret_key" name="post_secret_key" value="<%= post.uuid %>" />
		<input type="hidden" class="post_owner" class="post_owner" name="post_owner" value="1" />
	<%= submit_tag "Done" %>
<% end %>