<div id="post-header">
	<h1 id='post-title'><%= @post.title %></h1>
	<%= render :partial => "post_title", :locals => {:post => @post} %>
	
</div> <!-- EO #post-header -->

<ul id="steps-container">
	<% for step in @post.steps %>
		<%= render :partial => "step_edit", :locals => {:step => step }%>
	<% end %>
</ul>



