<div class='detail-wrapper'>
	<ul class="uploaded_pic_wrapper clearfix" id="pic-wrapper_<%= order %>">
		<% for detail in step.details %>
			<li>
				<%= image_tag(detail.photo.url(:small))%>
			</li>
		<% end %>
	</ul>
	
	<div class="uploaded_code_container">
		<% if step.detail_code %>
		<%= step.detail_code.code %>
		<% end %>
	</div>
	
	
	
</div>